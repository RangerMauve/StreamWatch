	params = { AllowScriptAccess: 'always' };
    function livestreamPlayerCallback(event) {
      if (event == 'ready') {
	player = document.getElementById("livestreamPlayer");
      	player.setDevKey('fxoES7aAk8QjELRX4fiMyuqOTaiuO8bfQ95EcOe6uzn_ZcSM3pXcWgeC03P54nBZXRhUlX5d0YZWuVIvBOG6dgtQ8S4oEcLht62SIp6rwn4');
